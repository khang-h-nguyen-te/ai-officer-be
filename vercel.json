{
  "version": 2,
  "builds": [
    {
      "src": "main.py",
      "use": "@vercel/python",
      "config": {
        "maxLambdaSize": "15mb",
        "runtime": "python3.11"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "main.py"
    }
  ],
  "env": {
    "PYTHONUNBUFFERED": "1",
    "MAX_MEMORY": "2048",
    "MAX_DURATION": "60",
    "VERCEL": "1"
  },
  "functions": {
    "main.py": {
      "memory": 2048,
      "maxDuration": 60
    }
  }
} 